import System;

class Component {
    public render(): VNode {
        return vdom <span>Hello from Component</span>;
    }
}

class App
{
    public render(): VNode {
        return vdom <span>
                            <Component></Component>
                            <b onClick={ -> System.alert("Hello World")}>Hello World</b> by LyraScript!
                    </span>;
    }

}
